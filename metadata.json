{
  "name": "puppetlabs-qe",
  "version": "0.0.1",
  "author": "puppetlabs",
  "summary": "Provides dependencies needed for using containers in CI at Puppet",
  "license": "Apache-2.0",
  "source": "https://github.com/puppetlabs/puppetlabs-qe",
  "project_page": "https://github.com/puppetlabs/puppetlabs-qe",
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 4.16.0 < 6.0.0"
    },
    {
      "name": "puppetlabs/git",
      "version_requirement": ">= 0.0.1 < 1.0.0"
    },
    {
      "name": "puppetlabs/apt",
      "version_requirement": ">= 6.0.0 < 7.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [
        "8"
      ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [
        "14.04",
        "16.04",
        "18.04"
      ]
    }
  ],
  "requirements": [
    {
      "name": "puppet",
      "version_requirement": ">= 4.8.0 < 7.0.0"
    }
  ]
}
